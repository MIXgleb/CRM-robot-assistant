      <Content>&lt;ToolboxItemSettings DisplayName="Проверить наличие файла/папки" InternalName="DoesItemExist" FriendlyName="Проверить существование файла или папки в файловой системе по указанному адресу" Width="150" Height="150" PathToImage="ExternalToolboxes\Image\ico_Files_Folders.png" PathToToolboxImage="{x:Null}" ImageType="NoImage" NoDelete="False" Invisible="False" Proportional="True" Version="51" Code="&amp;#xA;function DoesItemExist{{{block_id}}}&amp;#xA;{   &amp;#xA;    $__Error.Enter(&amp;quot;{{{block_native_id}}}&amp;quot;, &amp;quot;{{{ErrorLevel}}}&amp;quot;,&amp;quot;{{{ErrorDirection}}}&amp;quot;,&amp;quot;{{{ReportLevel}}}&amp;quot;,&amp;quot;{{{ErrorMessage}}}&amp;quot;);&amp;#xA;    $__Error.AddConnectors(&amp;quot;{{{connector-Error}}}&amp;quot;, &amp;quot;{{{connectorRight}}}&amp;quot;, &amp;quot;{{{connectorBottom}}}&amp;quot;);&amp;#xA;    &amp;#xA;    $thispath = {{{Path}}}; &amp;#xA;    $TypeItem    = &amp;quot;{{{TypeItem}}}&amp;quot;;&amp;#xA;    &amp;#xA;    try&amp;#xA;    {         &amp;#xA;        $thispath   = ExpandConstFilePath $thispath&amp;#xA;        $thisresult = $false;   &amp;#xA;            &amp;#xA;        if(-not $thisresult)&amp;#xA;        {&amp;#xA;            if($TypeItem -eq &amp;quot;File&amp;quot; -or $TypeItem -eq &amp;quot;Auto&amp;quot;)&amp;#xA;            {&amp;#xA;                $thisresult = [System.IO.File]::Exists($thispath);   &amp;#xA;            }  &amp;#xA;        }&amp;#xA;        &amp;#xA;        if(-not $thisresult)&amp;#xA;        {&amp;#xA;            if($TypeItem -eq &amp;quot;Directory&amp;quot; -or $TypeItem -eq &amp;quot;Auto&amp;quot;)&amp;#xA;            {&amp;#xA;                $thisresult = [System.IO.Directory]::Exists($thispath);  &amp;#xA;            } &amp;#xA;        }&amp;#xA;        &amp;#xA;        if($thisresult)&amp;#xA;        {       &amp;#xA;            $__Error.Jump(&amp;quot;{{{connector-Yes}}}&amp;quot;);&amp;#xA;            return;&amp;#xA;        }&amp;#xA;        else